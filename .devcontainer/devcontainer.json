{
	"name": "Rust development container",
	"image": "mcr.microsoft.com/devcontainers/rust:1",
	"features": {
		"ghcr.io/devcontainers-extra/features/apt-packages:1": {
			"packages": "libasound2-dev"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"fill-labs.dependi",
				"tamasfe.even-better-toml",
				"rust-lang.rust-analyzer"
			]
		}
	}
}
